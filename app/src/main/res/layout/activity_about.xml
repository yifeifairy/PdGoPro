<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/custom_id_app_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/custom_attr_app_bg"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">


    <include
        android:id="@+id/main_head"
        layout="@layout/view_head"
        android:layout_width="match_parent"
        android:layout_height="@dimen/navbar_height" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/layout_time_interval"
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="软件名称："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/tv_unit_time_interval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="自动腹膜透析设备医疗程序"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_threshold_value"
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="著作权人："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/tv_unit_threshold_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="福州东泽医疗器械有限公司"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_min_weight"
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="软件型号规格："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/tv_unit_min_weight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="PD-GO-Pro"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_allow_abdominal_volume"
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="软件发布版本："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="V2.1"
                android:id="@+id/tvVersion"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="软件完整版本："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/tv_unit_max_warning_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="V2.1.0.0"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />



        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="软件发布日期："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="2024年01月10日"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/useDeviceRl"
            android:layout_width="match_parent"
            android:layout_height="@dimen/input_box_bg_height_2"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
            android:layout_marginRight="@dimen/layout_input_box_bg_margin"
            android:layout_marginTop="5dp"
            android:background="?attr/custom_attr_input_box_bg"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="设备累计使用时间："
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/useDeviceTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="0小时"
                android:textColor="?attr/custom_attr_common_text_color"
                android:textSize="@dimen/input_box_tv_text_size" />



        </RelativeLayout>

    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            android:id="@+id/rootView"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navbar_btn_height"
            android:layout_alignParentBottom="true"
            layout="@layout/root_view" />
    </RelativeLayout>
</LinearLayout>