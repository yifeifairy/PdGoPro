                    android:id="@+id/et_prescription_item7"
                    android:layout_width="@dimen/input_box_et_width"
                    android:layout_height="@dimen/input_box_et_height"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/input_box_et_margin_right_2"
                    android:background="@null"
                    android:gravity="center_vertical|right"
                    android:maxLength="7"
                    android:textColor="@color/white"
                    android:textColorHint="@color/text_default_30"
                    android:textSize="@dimen/input_health_info_tv_value_text_size"
                    tools:ignore="SpeakableTextPresentCheck" />
            </RelativeLayout>

