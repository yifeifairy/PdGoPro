                    android:layout_width="@dimen/input_box_et_width"
                    android:layout_height="@dimen/input_box_et_height"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/input_box_et_margin_right_2"
                    android:background="@null"
                    android:gravity="center_vertical|right"
                    android:maxLength="7"
                    android:textColor="@color/white"
                    android:textColorHint="@color/text_default_30"
                    android:textSize="@dimen/input_health_info_tv_value_text_size"
                    tools:ignore="SpeakableTextPresentCheck" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/layout_prescription_item_4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/input_box_item_margin_left"
                android:layout_weight="1"
                android:background="@drawable/pa4"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/input_box_item_padding_left"
                android:paddingRight="@dimen/input_box_item_padding_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/input_title_prescription_item_4"
                    android:textColor="@color/white"
                    android:textSize="@dimen/input_health_info_tv_value_text_size" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/unit_min"
                    android:textColor="@color/white"
                    android:textSize="@dimen/input_health_info_tv_value_text_size" />

                <TextView
                    android:id="@+id/et_prescription_item4"
                    android:layout_width="@dimen/input_box_et_width"
                    android:layout_height="@dimen/input_box_et_height"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/input_box_et_margin_right_2"
                    android:background="@null"
                    android:gravity="center_vertical|right"
                    android:maxLength="7"
                    android:textColor="@color/white"
                    android:textColorHint="@color/text_default_30"
                    android:textSize="@dimen/input_health_info_tv_value_text_size"
                    tools:ignore="SpeakableTextPresentCheck" />


            </RelativeLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="13dp"
            android:orientation="horizontal">


            <RelativeLayout
                android:id="@+id/layout_prescription_item_5"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/input_box_item_margin_right"
                android:layout_weight="1"
                android:background="@drawable/pa5"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/input_box_item_padding_left"
                android:paddingRight="@dimen/input_box_item_padding_right">

                <TextView
                    android:layout_width="wrap_content"
