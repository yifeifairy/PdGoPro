    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.3"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/svTotalVolLl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="@dimen/input_box_item_margin_right"
            android:layout_marginBottom="@dimen/input_box_item_margin_right"
            android:layout_marginEnd="@dimen/input_box_item_margin_right"
            android:layout_marginStart="@dimen/treatment_countdown_margin"

            android:layout_weight="1"
            android:orientation="vertical"
            android:background="@drawable/gray_shape"
            android:paddingLeft="@dimen/treatment_countdown_margin"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="0.4"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_1"
                android:textColor="@color/white"
                android:textSize="@dimen/navbar_btn_text_size" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="0.6"
                android:layout_height="0dp" >

                <TextView
                    android:id="@+id/svTotalVolTv"
                    android:layout_width="0dp"
                    android:layout_weight="0.6"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center"
                    android:maxLength="7"
                    android:textColor="@color/white"
                    android:textColorHint="@color/text_default_30"
                    android:textSize="@dimen/input_health_info_tv_value_text_size"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:gravity="center"
                    android:layout_height="match_parent"
                    android:text="@string/unit_ml"
                    android:textColor="@color/white"
                    android:textSize="@dimen/navbar_btn_text_size" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
