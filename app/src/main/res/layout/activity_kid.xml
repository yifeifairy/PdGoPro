<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="?attr/custom_attr_app_bg"
    tools:context="com.emt.pdgo.next.ui.activity.apd.prescrip.KidActivity">

    <include
        layout="@layout/view_head" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/layout_input_box_bg_margin"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_marginRight="@dimen/layout_input_box_bg_margin"
        android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/totalVolRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_1"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/totalVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/treatment_countdown_margin"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/totalVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/totalVolUnit"
                android:textSize="@dimen/input_box_tv_text_size" />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/cycleVolRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_weight="1"
            android:background="@drawable/pa20"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_2"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/cycleVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

            <TextView
                android:id="@+id/cycleVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/cycleVolUnit"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">
        <RelativeLayout
            android:id="@+id/cycleNumRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_3"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/cycleNumUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/treatment_countdown_margin"
                android:text="次"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/cycleNumTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/cycleNumUnit"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/retainTimeRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_weight="1"
            android:background="@drawable/pa20"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_4"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/retainTimeUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="分"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/retainTimeTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/retainTimeUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">
        <RelativeLayout
            android:id="@+id/finalRetainVolRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_5"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/finalRetainVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/treatment_countdown_margin"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/finalRetainVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/finalRetainVolUnit"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/lastRetainVolRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_weight="1"
            android:background="@drawable/pa20"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_6"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/lastRetainVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/lastRetainVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/lastRetainVolUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:layout_height="match_parent" >
                <CheckBox
                    android:id="@+id/finalSupplyCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="末袋补液"
                    android:textColor="@color/white"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:theme="@style/CheckBoxTheme" />

                <TextView
                    android:text="(通道1)"
                    android:textColor="@color/white"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:layout_marginLeft="@dimen/treatment_countdown_margin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/finalSupplyRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa9"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_9"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/finalSupplyUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/treatment_countdown_margin"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/finalSupplyTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/finalSupplyUnit"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/ultVolRl"
            android:layout_width="0dp"
            android:layout_height="@dimen/preparation_iv_info_height"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_weight="1"
            android:background="@drawable/pa20"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_8"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/ultVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/ultVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/ultVolUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/preparation_iv_info_height"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:id="@+id/firstPerVolRl"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/input_title_prescription_item_7"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/firstPerVolUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="@string/unit_ml"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/firstPerVolTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/firstPerVolUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_weight="1"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right"
            android:id="@+id/bsaRl"
            android:background="@drawable/pa20" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="体表面积(BSA)："
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/bsaUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="m²"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/bsaTv"
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/bsaUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/preparation_iv_info_height"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:id="@+id/heightRl"
            android:background="@drawable/pa12"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="身高："
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/heightUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="cm"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:id="@+id/heightTv"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/heightUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_height="match_parent">

            <LinearLayout
                android:background="@drawable/pa20"
                android:layout_width="match_parent"
                android:gravity="center"
                android:layout_height="match_parent" >
                <CheckBox
                    android:theme="@style/CheckBoxTheme"
                    android:id="@+id/checkbox"
                    android:layout_centerVertical="true"
                    android:text="输入体重等信息"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/preparation_iv_info_height"
        android:layout_marginTop="@dimen/treatment_countdown_margin"
        android:id="@+id/sexLl"
        android:layout_gravity="center_vertical">

        <RelativeLayout
            android:layout_width="0dp"
            android:id="@+id/weightRl"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/pa12" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="体重："
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:id="@+id/weightUnit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="10dp"
                android:text="@string/unit_kg"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
            <TextView
                android:layout_width="@dimen/input_box_et_width"
                android:gravity="center"
                android:id="@+id/weightTV"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@id/weightUnit"
                android:layout_centerVertical="true"
                android:textColor="@color/white"
                android:textSize="@dimen/input_box_tv_text_size" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="@drawable/pa20"
            android:paddingLeft="@dimen/input_box_item_padding_left"
            android:paddingRight="@dimen/input_box_item_padding_right"
            android:layout_marginLeft="@dimen/treatment_countdown_margin"
            android:layout_height="match_parent" >

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="性别："
                android:textSize="@dimen/input_box_tv_text_size"
                android:textColor="@color/white"
                android:layout_height="wrap_content" />

            <RadioGroup
                android:layout_width="0dp"
                android:layout_weight="3"
                android:id="@+id/radioGroup"
                android:orientation="horizontal"
                android:layout_height="match_parent">

                <RadioButton
                    android:id="@+id/maleChecked"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="男"
                    android:buttonTint="@color/white"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:textColor="@color/white"
                    android:layout_height="match_parent" />
                <RadioButton
                    android:id="@+id/femaleChecked"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="女"
                    android:buttonTint="@color/white"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:textColor="@color/white"
                    android:layout_height="match_parent" />
                <RadioButton
                    android:id="@+id/secrecyChecked"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="保密"
                    android:buttonTint="@color/white"
                    android:textSize="@dimen/input_box_tv_text_size"
                    android:textColor="@color/white"
                    android:layout_height="match_parent" />

            </RadioGroup>

        </LinearLayout>

    </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <include
            android:id="@+id/rootView"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navbar_btn_height"
            android:layout_centerVertical="true"
            layout="@layout/root_view" />
        <com.emt.pdgo.next.ui.view.StateButton
            android:id="@+id/btnNext"
            android:layout_width="@dimen/custom_main_btn_re_selfcheck_width"
            android:layout_height="@dimen/custom_main_btn_re_selfcheck_height"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="end"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
